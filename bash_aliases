alias ls="ls -h"
alias h='history'
alias cls='clear'
alias j='jobs -l'
alias rm='rm -i'
alias df="df -h"
alias du="du -h"
alias tailf='tail -f'
alias sc='starcluster'
alias scssh='starcluster sn -u $SC_USER -X '
alias screen='export SCREENPWD=$(pwd); /usr/bin/screen'
